@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center" style="height:100px;">
    <button type="button" class="btn btn-primary" style="float:left; margin:10px; background-color:lavender;">Sobre nós</button>
    <button type="button" class="btn btn-primary" style="float:left; margin:10px; background-color:lavender;">Contactos</button>
    <button type="button" class="btn btn-primary" style="float:left; margin:10px; background-color:lavender;">Localização</button>
    <button id="signInBtn" type="button" class="btn btn-primary" style="float:right; margin:10px; background-color:lavender;">Entrar</button>
</div>
<div style="height:100px;">
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="d-block w-100" src="..." alt="First slide">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="..." alt="Second slide">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="..." alt="Third slide">
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

</div>
<div>
    <button>Reservar já!</button>
    <button>Temas</button>
</div>
<div>
    
    <button type="button" class="btn btn-secondary">Secondary</button>
    <button type="button" class="btn btn-success">Success</button>
    <button type="button" class="btn btn-danger">Danger</button>
    <button type="button" class="btn btn-warning">Warning</button>
    <button type="button" class="btn btn-info">Info</button>
    <button type="button" class="btn btn-light">Light</button>
    <button type="button" class="btn btn-dark">Dark</button>

    <button type="button" class="btn btn-link">Link</button>
</div>
